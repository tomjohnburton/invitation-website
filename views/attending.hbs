<div>

  <table class="table table-dark">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Adults</th>
        <th scope="col">Children</th>
        <th scope="col">Camping</th>
        <th scope="col">Days Attending</th>
        <th scope="col">Song Choice</th>
      </tr>
    </thead>
    <tbody>
      {{#each response}}
      <tr>
        <td>{{name}}</td>
        <td>{{adults}}</td>
        <td>{{children}}</td>
        <td>{{camping}}</td>
        <td>{{daysAttending}}</td>
        <td>{{favoriteSong}}</td>
      </tr>
      {{/each}}

    </tbody>
  </table>
</div>